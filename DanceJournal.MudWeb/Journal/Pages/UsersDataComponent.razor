@page "/users"
<MudContainer Class="mt-16 px-8" MaxWidth="MaxWidth.False">
    <MudGrid>
        <MudItem xs="12" sm="12">
            <MudTable T="User" Items="@_users">
                <HeaderContent>
                    <MudTh>Id</MudTh>
                    <MudTd>Surname</MudTd>
                    <MudTd>FirstName</MudTd>
                    <MudTd>SecondName</MudTd>
                    <MudTd>BirthDate</MudTd>
                    <MudTh>Gender</MudTh>
                    <MudTh>Email</MudTh>
                    <MudTh>Phone Number</MudTh>
                    <MudTh>Level</MudTh>
                    <MudTh>Role</MudTh>
                    <MudTh>Subscription</MudTh>
                    <MudTh>Salary</MudTh>
                </HeaderContent>
                <RowTemplate>
                    <MudTd>@context.Id</MudTd>
                        <MudTd>@context.Surname</MudTd>
                        <MudTd>@context.FirstName</MudTd>
                        <MudTd>@context.SecondName</MudTd>
                        <MudTd>@context.BirthDate</MudTd>
                        <MudTd>@context.Gender</MudTd>
                        <MudTd>@context.Email</MudTd>
                        <MudTd>@context.PhoneNumber</MudTd>
                        <MudTd>@context.Level.Title</MudTd>
                        <MudTd>@context.Role.Name</MudTd>
                        <MudTd>@context.Subscription.SubscriptionType.Name</MudTd>
                        <MudTd>@context.Salary</MudTd>
                    </RowTemplate>
                </MudTable>

            </MudItem>
        </MudGrid>
    </MudContainer>
